---
export const prerender = true;
import profile from '../assets/profile.png';
import '../styles/global.css';
---
<html lang="en-US">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width">
    <link rel="icon" type="apple-touch-icon" href={profile} />
    <link rel="icon" type="image/png" href={profile} />
    <link rel="alternate" type="application/feed+json" href="/feed.json" />
    <link rel="alternate" type="application/json" href="/feed.json" />
  </head>
  <a href="/">
    <picture>
      <img src={profile} alt="Trevor Opiyo Profile Photo" href="/" rel="prefetch"/>
    </picture>
  </a>
  <br/>
  <a href="/" alt="trevoropiyo.com: Home Page" rel="prefetch">
    <h1>
      Trevor Opiyo
    </h1>
  </a>
  <hr/>
  <nav>
    <a href="/" rel="prefetch" alt="Home Page">Home  </a>
    &bull;
    <a href="/blog/" alt="Blog Page" rel="prefetch">  Blog</a>
    &bull;
    <a href="/about/" alt="About Page" rel="prefetch">  About</a>
  </nav>
  <hr/>
  <br/>
</html>
