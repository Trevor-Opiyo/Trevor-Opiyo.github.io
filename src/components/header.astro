---
export const prerender = true;
import profile from '../assets/profile.png';
---
<html lang="en-US">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width">
    <title> Trevor Opiyo: trevropiyo.com</title>
    <link rel="icon" type="apple-touch-icon" href={profile} />
    <link rel="icon" type="image/png" href={profile} />
    <link rel="alternate" type="application/feed+json" href="/feed.json" />
    <link rel="alternate" type="application/json" href="/feed.json" />
  </head>
  <a href="/">
    <picture>
      <img src={profile} alt="Trevor Opiyo Profile Photo" href="/" rel="prefetch"/>
    </picture>
  </a>
  <br />
  <a href="/" alt="Trevor Opiyo" rel="prefetch">
    <h1 >
      Trevor Opiyo
    </h1>
  </a>
  <hr />
  <nav>
    <a href="/">Home </a>
    &bull;
    <a href="/blog/"> Blog</a>
    &bull;
    <a href="/about/"> About </a>
  </nav>
